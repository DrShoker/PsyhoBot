<table class="table table-hover">
    <tr>
        <th class="active">ID</th>
        <th class="active">Date</th>
        <th class="active">PatientId</th>
        <th class="active"></th>
    </tr>
    @foreach (var dl in ViewBag.result)
    {
        <tr>
            <th>@dl.Id</th>
            <th>@dl.Date</th>
            <th>@dl.PatientId</th>
            <th>
                @using (@Html.BeginForm("Delete", "Dialog", FormMethod.Post))
                {
                    <input type="hidden" name="id" value="@dl.Id" />
                    <input type="submit" value="Delete" class="btn btn-danger" />
                }
            </th>
        </tr>
    }
</table>
